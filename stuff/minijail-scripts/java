# Exit
exit: 1
exit_group: 1

# I/O
access: 1
close: 1
fcntl: arg1 == F_GETFD || arg1 == F_SETFD && arg2 == FD_CLOEXEC
fstat: 1
getdents64: 1
getdents: 1
lseek: 1
lstat: 1
open: 1
openat: 1
read: 1
readlink: 1
socket: return ENETDOWN
stat: 1
write: 1

# Environment
getcwd: 1
geteuid: 1
getrlimit: 1
getuid: 1
setrlimit: arg0 == RLIMIT_STACK; return EPERM
uname: 1

# Threads
clone: arg0 == CLONE_VM|CLONE_FS|CLONE_FILES|CLONE_SIGHAND|CLONE_THREAD|CLONE_SYSVSEM|CLONE_SETTLS|CLONE_PARENT_SETTID|CLONE_CHILD_CLEARTID
futex: 1
kill: arg0 == 0 && arg1 == SIGCHLD
sched_getaffinity: 1
sched_yield: 1
set_robust_list: 1

# Time
clock_getres: 1
clock_gettime: 1
gettimeofday: 1
time: 1

# Memory
brk: 1
madvise: 1
mmap: 1
mprotect: 1
munmap: 1

# Signals
gettid: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
