# Exit
exit: 1
exit_group: 1

# I/O
close: 1
dup: 1
fcntl: arg1 == F_GETFL
fstat: 1
lseek: 1
open: 1
read: 1
select: 1
socket: return ENETDOWN
write: 1

# arg1 == SNDCTL_TMR_TIMEBASE or SNDRV_TIMER_IOCTL_NEXT_DEVICE or TCGETS
ioctl: 1

# Time
gettimeofday: 1
clock_gettime: 1
timer_create: 1
timer_settime: 1
timer_delete: 1

# Memory
brk: 1
mmap: 1
mremap: 1
munmap: 1

# Signals
rt_sigreturn: 1
rt_sigaction: 1
rt_sigprocmask: 1
