# Exit
exit: 1
exit_group: 1

# I/O
close: 1
dup: 1
dup2: 1
dup3: 1
fcntl: arg1 == F_GETFL
fstat: 1
lseek: 1
open: 1
read: 1
socket: return ENETDOWN
write: 1
writev: 1

# Memory
brk: 1
futex: return ENOSYS
mmap: 1
mremap: 1
munmap: 1
mprotect: 1

# Time
clock_gettime: 1
gettimeofday: 1
time: 1
times: 1

# abort()
getpid: 1
gettid: 1
rt_sigaction: arg0 != SIGSYS
rt_sigprocmask: 1
rt_sigreturn: 1
tgkill: arg0 == 2
